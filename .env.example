# .env.example

# LiveKit server URL
LIVEKIT_URL="wss://<YOUR_LIVEKIT_SERVER>.livekit.cloud"

# LiveKit API credentials
LIVEKIT_API_KEY="<YOUR_LIVEKIT_API_KEY>"
LIVEKIT_API_SECRET="<YOUR_LIVEKIT_API_SECRET>"

# OpenAI API Key for real-time LLM processing
OPENAI_API_KEY="<YOUR_OPENAI_API_KEY>"

# SIP Outbound Trunk ID (for making outbound phone calls via Twilio)
SIP_OUTBOUND_TRUNK_ID="<YOUR_SIP_OUTBOUND_TRUNK_ID>"
